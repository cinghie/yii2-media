{
    "name": "cinghie/yii2-media",
    "description": "Yii2 Media to manage a files on Yii2 site.",
    "keywords": [
        "yii2 media", 
        "yii2 attachments",
        "yii2 upload",
        "yii2 media wordpress"
    ],
    "homepage": "https://github.com/cinghie/yii2-media",
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Gogodigital.it",
            "email": "info@gogodigital.it",
            "homepage": "http://www.gogodigital.it/"
        }
    ],
    "require": {
        "yiisoft/yii2": "~2.0.14",
        "cinghie/yii2-masonry": "@dev",
        "cinghie/yii2-tinify": "@dev",
        "cinghie/yii2-traits": "@dev"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "psr-4": {
            "cinghie\\media\\": ""
        }
    },
    "extra": {
        "bootstrap": "cinghie\\media\\Bootstrap"
    }
}
